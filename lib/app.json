{
  "app_id": "agendor",
  "name": "Agendor",
  "color": "#27385F",
  "description": {
    "pt_BR": "O Agendor é uma plataforma de aprimoramento em vendas que ajuda milhares de vendedores a alcançarem todo o seu potencial.",
    "en": ""
  },
  "website": "https://www.agendor.com.br",
  "signup_url": "https://www.agendor.com.br/entrar",
  "api_base_uri": {
    "production": {
      "uri": "https://api.agendor.com.br/v3",
      "label": "Produção",
      "default": true
    }
  },
  "authentication": {
    "type": "pass_through_header",
    "fields": [
      {
        "name": "token",
        "label": "Token para usar a API do Agendor",
        "mapping": "Authorization",
        "prefix": "Token",
        "type": "text"
      }
    ],
    "ping_request": {
      "method": "/users/me"
    }
  }
}
